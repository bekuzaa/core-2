cluded LICENSE.md file for more details.","maintainers":[{"name":"rvagg","email":"rod@vagg.org"}],"time":{"modified":"2022-06-27T22:44:37.806Z","created":"2015-03-29T03:03:02.141Z","1.0.0":"2015-03-29T03:03:02.141Z","1.0.1":"2015-07-10T00:45:48.113Z"},"homepage":"https://github.com/rvagg/tsml","keywords":["template","templatestring","es6"],"repository":{"type":"git","url":"git+https://github.com/rvagg/tsml.git"},"author":{"name":"Rod Vagg","email":"rod@vagg.org"},"bugs":{"url":"https://github.com/rvagg/tsml/issues"},"license":"MIT","readmeFilename":"README.md","users":{}}