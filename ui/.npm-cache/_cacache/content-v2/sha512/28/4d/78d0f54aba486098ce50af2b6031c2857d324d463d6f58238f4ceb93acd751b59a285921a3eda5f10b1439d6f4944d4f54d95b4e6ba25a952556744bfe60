ate = this.state || {};\n\n    return !(this.updateOnProps || Object.keys({...nextProps, ...this.props})).every((p) => is(nextProps[p], this.props[p]))\n      || !(this.updateOnStates || Object.keys({ ...nextState, ...state})).every((s) => is(nextState[s], state[s]));\n  }\n}\n\nexport default ImmutablePureComponent;\n```\n","maintainers":[{"name":"mastermonar","email":"dev@monarski.pl"}],"time":{"modified":"2022-06-25T23:45:00.259Z","created":"2017-01-18T22:03:36.684Z","0.0.1":"2017-01-18T22:03:36.684Z","0.0.2":"2017-02-14T15:41:36.795Z","0.0.3":"2017-02-15T15:37:22.680Z","0.0.4":"2017-02-15T15:50:31.145Z","0.0.5":"2017-06-01T16:03:25.169Z","1.0.0":"2017-06-15T15:38:56.406Z","1.0.1":"2017-07-26T14:31:54.336Z","1.1.0":"2017-10-02T15:42:03.870Z","1.1.1":"2017-10-23T19:44:37.141Z","1.1.2":"2018-06-14T15:23:21.319Z","1.2.0":"2018-06-15T08:01:38.178Z","1.2.1":"2018-06-26T11:14:42.993Z","2.0.0":"2018-06-26T12:22:57.128Z","1.2.2":"2018-06-27T08:34:49.593Z","1.2.3":"2018-08-03T12:31:58.946Z","2.0.1":"2018-12-31T15:54:23.442Z","2.1.0":"2019-01-01T22:34:14.446Z","2.2.0-rc.0":"2019-01-30T23:50:57.866Z","2.2.0-rc.1":"2019-01-31T11:36:27.778Z","2.2.0-rc.2":"2019-01-31T12:11:47.210Z","2.2.0-rc.3":"2019-02-17T19:42:08.990Z","2.2.0-rc.4":"2019-02-18T21:09:12.515Z","2.2.0":"2019-02-19T20:06:17.607Z","2.2.1":"2019-02-19T20:14:26.435Z","2.2.2":"2019-11-06T22:57:00.101Z","1.2.4":"2020-12-13T11:47:03.613Z"},"homepage":"https://github.com/Monar/react-immutable-pure-component#readme","keywords":["react","immutable","pure","component","PureComponent"],"repository":{"type":"git","url":"git+https://github.com/Monar/react-immutable-pure-component.git"},"author":{"name":"Piotr Tomasz Monarski"},"bugs":{"url":"https://github.com/Monar/react-immutable-pure-component/issues"},"license":"MIT","readmeFilename":"README.md","users":{"genbuhase":true}}